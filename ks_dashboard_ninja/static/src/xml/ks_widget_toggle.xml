            <div class="ks_select_icon_div">
                <input id="ks_custom_icon" type="radio" name="default_custom_icon_toggle" class="ks_toggle_icon_input"
                       value="Custom"/> Upload Icon
            </div>
        </div>
    </t>
</templates>
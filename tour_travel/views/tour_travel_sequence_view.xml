<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">


        <record id="seq_crm_lead_1" model="ir.sequence">
            <field name="name">Crm Lead</field>
            <field name="code">crm.lead</field>
            <field name="prefix">LD</field>
            <field name="padding">6</field>
        </record>
		
		
        <record id="sequence_tour_hotel_book1" model="ir.sequence">
            <field name="name">Tour Hotel Book</field>
            <field name="code">tour.hotel.reservation</field>
            <field name="prefix">TH</field>
            <field name="padding">6</field>
        </record>
		
		
		  <!--
         ========================================================= 
        	Sequence number of Transport Booking
         =========================================================
         -->	
        
        <record id="sequence_transport_booking" model="ir.sequence">
            <field name="name">Transport Booking</field>
            <field name="code">transport.book</field>
            <field name="prefix">TR</field>
            <field name="padding">6</field>
        </record>
		
		
		 <!--
         ========================================================= 
        	Sequence number of Tour BOOK
         =========================================================
         -->	
        
        <record id="sequence_tour_booking" model="ir.sequence">
            <field name="name">Tour Booking</field>

<odoo>
    <record id="ir_cron_subscription_autocomplete" model="ir.cron">
        <field name="name">Subscription Autocomplete : Create opportunity with services about to expire</field>
        <field name="model_id" ref="model_sale_subscription"/>
        <field name="state">code</field>
        <field name="code">model.start_create_opportunity(30)</field>
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="numbercall">-1</field>
        <field eval="False" name="doall"/>
    </record>
</odoo>

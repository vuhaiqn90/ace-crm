<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record id="action_crm_caresoft_connector" model="ir.actions.client">
            <field name="name">Dashboard</field>
            <field name="tag">ace_crm_caresoft.embed_frame</field>
        </record>

        <menuitem id="menu_crm_caresoft_connector"
                  name="Contact Center"
                  action="action_crm_caresoft_connector"
                  groups="base.group_user"
                  web_icon="ace_crm_caresoft,static/description/modules.png"/>
                  sequence="1"/>

        <template id="assets_backend" name="Crm Caresoft Connector assets" inherit_id="web.assets_backend">
            <xpath expr="." position="inside">
                <script type="text/javascript" src="/ace_crm_caresoft/static/src/js/crm_caresoft.js"/>
                <link rel="stylesheet" href="/ace_crm_caresoft/static/src/css/crm_caresoft.css"/>
            </xpath>
        </template>
    </data>
</odoo>